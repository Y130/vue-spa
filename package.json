{
  "name": "vue-spa",
  "version": "1.0.0",
  "description": "a vue spa app",
  "scripts": {
    "dev": "cross-env env_config=dev node build/dev-server.js",
    "dev:mock": "cross-env NODE_MOCK=mock env_config=dev node build/dev-server.js",
    "build": "npm run build:test && npm run build:sit && npm run build:prod",
    "build:prod": "rimraf dist/prod/* && cross-env NODE_ENV=production env_config=prod node build/build.js",
    "build:sit": "rimraf dist/sit/* && cross-env NODE_ENV=production env_config=sit node build/build.js",
    "build:test": "rimraf dist/test/* && cross-env NODE_ENV=production env_config=test node build/build.js",
    "lint": "eslint --fix --ext .js,.vue src",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run lint && npm run unit && npm run e2e"
  },
  "keywords": [
    "vue",
    "spa"
  ],
  "author": "yilei <github@qq.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "fastclick": "^1.0.6",
    "font-awesome": "4.7.0",
    "js-cookie": "^2.2.0",
    "jsonlint": "^1.6.2",
    "jszip": "^3.1.5",
    "mockjs": "^1.0.1-beta3",
    "normalize.css": "^7.0.0",
    "nprogress": "^0.2.0",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
